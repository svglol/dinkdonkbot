{
  "name": "dinkdonkbot",
  "type": "module",
  "version": "1.0.0",
  "private": true,
  "packageManager": "pnpm@10.28.2",
  "description": "A discord bot that posts notifications when twitch streamers go live",
  "author": "Matthew Trotman <savageguy@gmail.com>",
  "license": "MIT",
  "keywords": [],
  "main": "src/server.ts",
  "scripts": {
    "start": "tsx src/server.ts",
    "dev": "wrangler dev src/server.ts --test-scheduled",
    "localtunnel": "npx localtunnel --port 8787 --subdomain dinkdonkbotdev",
    "ngrok": "ngrok http 8787",
    "fix": "eslint --fix .",
    "lint": "eslint .",
    "typecheck": "tsc --noEmit",
    "test": "vitest run",
    "register": "tsx src/discord/register.ts",
    "publish": "wrangler deploy",
    "db:generate": "drizzle-kit generate",
    "db:migrations": "wrangler d1 migrations apply DB --remote",
    "db:migrations-local": "wrangler d1 migrations apply DB"
  },
  "dependencies": {
    "@discordjs/formatters": "0.6.2",
    "@discordjs/rest": "2.6.0",
    "date-fns-tz": "3.2.0",
    "discord-api-types": "0.38.38",
    "discord-interactions": "4.4.0",
    "drizzle-orm": "0.45.1",
    "itty-router": "5.0.22",
    "tz-lookup": "6.1.25"
  },
  "devDependencies": {
    "@antfu/eslint-config": "7.4.3",
    "@cloudflare/vitest-pool-workers": "0.12.8",
    "@cloudflare/workers-types": "4.20260131.0",
    "@types/tz-lookup": "6.1.2",
    "dotenv": "17.2.3",
    "drizzle-kit": "0.31.8",
    "eslint": "9.36.0",
    "ngrok": "5.0.0-beta.2",
    "tsx": "4.21.0",
    "typescript": "5.9.3",
    "vite-tsconfig-paths": "5.1.4",
    "vitest": "3.2.4",
    "wrangler": "4.38.0"
  }
}
