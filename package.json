{
  "name": "dinkdonkbot",
  "type": "module",
  "version": "1.0.0",
  "private": true,
  "description": "A discord bot that posts notifications when twitch streamers go live",
  "author": "Matthew Trotman <savageguy@gmail.com>",
  "license": "MIT",
  "keywords": [],
  "main": "src/server.ts",
  "scripts": {
    "start": "tsx src/server.ts",
    "dev": "wrangler dev src/server.ts",
    "ngrok": "ngrok http 8787",
    "fix": "eslint --fix .",
    "lint": "eslint .",
    "register": "tsx src/register.ts",
    "publish": "wrangler deploy",
    "db:generate": "drizzle-kit generate:sqlite",
    "db:migrations": "wrangler d1 migrations apply DB --remote",
    "db:migrations-local": "wrangler d1 migrations apply DB"
  },
  "dependencies": {
    "discord-interactions": "3.4.0",
    "drizzle-orm": "0.30.8",
    "itty-router": "5.0.12"
  },
  "devDependencies": {
    "@antfu/eslint-config": "2.13.3",
    "@cloudflare/workers-types": "4.20240405.0",
    "dotenv": "16.4.5",
    "drizzle-kit": "0.20.14",
    "eslint": "9.0.0",
    "ngrok": "5.0.0-beta.2",
    "tsx": "4.7.2",
    "wrangler": "3.50.0"
  }
}
