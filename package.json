{
  "name": "dinkdonkbot",
  "type": "module",
  "version": "1.0.0",
  "private": true,
  "packageManager": "pnpm@9.6.0",
  "description": "A discord bot that posts notifications when twitch streamers go live",
  "author": "Matthew Trotman <savageguy@gmail.com>",
  "license": "MIT",
  "keywords": [],
  "main": "src/server.ts",
  "scripts": {
    "start": "tsx src/server.ts",
    "dev": "wrangler dev src/server.ts --test-scheduled",
    "ngrok": "ngrok http 8787",
    "fix": "eslint --fix .",
    "lint": "eslint .",
    "typecheck": "tsc --noEmit",
    "test": "vitest run",
    "register": "tsx src/register.ts",
    "publish": "wrangler deploy",
    "db:generate": "drizzle-kit generate:sqlite",
    "db:migrations": "wrangler d1 migrations apply DB --remote",
    "db:migrations-local": "wrangler d1 migrations apply DB"
  },
  "dependencies": {
    "discord-interactions": "3.4.0",
    "drizzle-orm": "0.32.1",
    "itty-router": "5.0.17"
  },
  "devDependencies": {
    "@antfu/eslint-config": "2.23.2",
    "@cloudflare/vitest-pool-workers": "0.4.16",
    "@cloudflare/workers-types": "4.20240725.0",
    "dotenv": "16.4.5",
    "drizzle-kit": "0.20.18",
    "eslint": "9.7.0",
    "ngrok": "5.0.0-beta.2",
    "tsx": "4.16.2",
    "typescript": "5.5.4",
    "vitest": "1.3.0",
    "wrangler": "3.65.1"
  }
}
