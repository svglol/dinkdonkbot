{
  "name": "dinkdonkbot",
  "type": "module",
  "version": "1.0.0",
  "private": true,
  "packageManager": "pnpm@9.15.9",
  "description": "A discord bot that posts notifications when twitch streamers go live",
  "author": "Matthew Trotman <savageguy@gmail.com>",
  "license": "MIT",
  "keywords": [],
  "main": "src/server.ts",
  "scripts": {
    "start": "tsx src/server.ts",
    "dev": "wrangler dev src/server.ts --test-scheduled",
    "ngrok": "ngrok http 8787",
    "fix": "eslint --fix .",
    "lint": "eslint .",
    "typecheck": "tsc --noEmit",
    "test": "vitest run",
    "register": "tsx src/discord/register.ts",
    "publish": "wrangler deploy",
    "db:generate": "drizzle-kit generate",
    "db:migrations": "wrangler d1 migrations apply DB --remote",
    "db:migrations-local": "wrangler d1 migrations apply DB"
  },
  "dependencies": {
    "discord-interactions": "4.3.0",
    "drizzle-orm": "0.44.2",
    "itty-router": "5.0.18"
  },
  "devDependencies": {
    "@antfu/eslint-config": "4.14.1",
    "@cloudflare/vitest-pool-workers": "0.8.38",
    "@cloudflare/workers-types": "4.20250617.0",
    "dotenv": "16.5.0",
    "drizzle-kit": "0.31.1",
    "eslint": "9.29.0",
    "ngrok": "5.0.0-beta.2",
    "tsx": "4.20.3",
    "typescript": "5.8.3",
    "vitest": "3.2.3",
    "wrangler": "3.114.9"
  }
}
