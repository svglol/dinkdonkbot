    "lint:style": "stylelint **/*.{vue,css} --fix --ignore-path .gitignore",
    "typecheck": "nuxt typecheck"
  },
  "dependencies": {
    "@antfu/eslint-config": "4.8.1",
    "@nuxt/content": "3.3.0",
    "@nuxt/eslint": "1.2.0",
    "@nuxt/fonts": "0.11.0",
    "@nuxt/ui": "2.21.1",
    "@unhead/vue": "2.0.0-rc.9",
    "@vueuse/core": "11.3.0",
    "@vueuse/nuxt": "11.3.0",
    "eslint": "9.22.0",
    "eslint-plugin-tailwindcss": "3.18.0",
    "nuxt": "3.16.0",
    "nuxt-og-image": "5.0.1",
    "postcss-html": "1.8.0",
    "stylelint": "16.15.0",
    "stylelint-config-recommended-vue": "1.6.0",
    "stylelint-config-standard": "37.0.0",
    "unstorage": "1.15.0",
    "vue": "3.5.13",
    "vue-router": "4.5.0",
    "vue-tsc": "2.2.8"
