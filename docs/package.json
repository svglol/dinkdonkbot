    "@nuxt/ui": "2.22.1",
    "@unhead/vue": "2.0.12",
    "@vueuse/core": "11.3.0",
    "@vueuse/nuxt": "11.3.0",
    "eslint": "9.31.0",
    "eslint-plugin-tailwindcss": "3.18.0",
    "nuxt": "3.17.7",
    "nuxt-og-image": "5.1.9",
    "postcss-html": "1.8.0",
    "stylelint": "16.21.1",
    "stylelint-config-recommended-vue": "1.6.1",
    "stylelint-config-standard": "37.0.0",
    "unstorage": "1.16.1",
