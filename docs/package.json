{
  "name": "nuxt-app",
  "type": "module",
  "private": true,
  "packageManager": "pnpm@9.15.7",
  "scripts": {
    "build": "nuxt build",
    "dev": "nuxt dev",
    "generate": "nuxt generate --preset github_pages",
    "preview": "nuxt preview",
    "postinstall": "nuxt prepare",
    "lint": "pnpm lint:js && pnpm lint:style",
