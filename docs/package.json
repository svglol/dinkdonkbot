    "@nuxt/ui": "3.3.0",
    "@unhead/vue": "2.0.14",
    "@vueuse/core": "13.6.0",
    "@vueuse/nuxt": "13.6.0",
    "better-sqlite3": "12.2.0",
    "eslint": "9.32.0",
    "eslint-plugin-better-tailwindcss": "3.7.4",
    "nuxt": "4.0.2",
    "nuxt-og-image": "5.1.9",
    "postcss-html": "1.8.0",
    "stylelint": "16.23.1",
    "stylelint-config-recommended-vue": "1.6.1",
    "stylelint-config-standard": "39.0.0",
